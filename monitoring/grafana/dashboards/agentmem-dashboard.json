{
  "dashboard": {
    "title": "AgentMem System Dashboard",
    "tags": ["agentmem", "monitoring", "performance"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "Request Rate",
        "type": "graph",
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(agentmem_requests_total[5m])",
            "legendFormat": "{{method}} {{endpoint}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(agentmem_errors_total[5m])",
            "legendFormat": "{{error_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "errps", "label": "Errors/sec"},
          {"format": "short"}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [10], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "60s",
          "handler": 1,
          "name": "High Error Rate",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 3,
        "title": "Request Duration (p50, p95, p99)",
        "type": "graph",
        "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(agentmem_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(agentmem_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(agentmem_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "title": "Active Connections",
        "type": "graph",
        "gridPos": {"x": 12, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "agentmem_active_connections",
            "legendFormat": "Active Connections",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Connections"},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "title": "Memory Operations",
        "type": "graph",
        "gridPos": {"x": 0, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(agentmem_memory_operations_total{operation=\"create\"}[5m])",
            "legendFormat": "Create",
            "refId": "A"
          },
          {
            "expr": "rate(agentmem_memory_operations_total{operation=\"read\"}[5m])",
            "legendFormat": "Read",
            "refId": "B"
          },
          {
            "expr": "rate(agentmem_memory_operations_total{operation=\"update\"}[5m])",
            "legendFormat": "Update",
            "refId": "C"
          },
          {
            "expr": "rate(agentmem_memory_operations_total{operation=\"delete\"}[5m])",
            "legendFormat": "Delete",
            "refId": "D"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Operations/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 6,
        "title": "Cache Hit Rate",
        "type": "singlestat",
        "gridPos": {"x": 12, "y": 16, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "rate(agentmem_cache_hits_total[5m]) / (rate(agentmem_cache_hits_total[5m]) + rate(agentmem_cache_misses_total[5m])) * 100",
            "refId": "A"
          }
        ],
        "format": "percent",
        "valueName": "current",
        "thresholds": "80,90",
        "colors": ["#d44a3a", "#e0b400", "#299c46"]
      },
      {
        "id": 7,
        "title": "Database Connection Pool",
        "type": "singlestat",
        "gridPos": {"x": 18, "y": 16, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "agentmem_db_connections_active",
            "refId": "A"
          }
        ],
        "format": "short",
        "valueName": "current",
        "thresholds": "40,45",
        "colors": ["#299c46", "#e0b400", "#d44a3a"]
      },
      {
        "id": 8,
        "title": "Tool Execution Duration",
        "type": "graph",
        "gridPos": {"x": 12, "y": 20, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(agentmem_tool_execution_duration_seconds_bucket[5m]))",
            "legendFormat": "{{tool_name}} p95",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 9,
        "title": "LLM API Calls",
        "type": "graph",
        "gridPos": {"x": 0, "y": 24, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(agentmem_llm_calls_total[5m])",
            "legendFormat": "{{provider}} {{model}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Calls/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 10,
        "title": "System Resource Usage",
        "type": "graph",
        "gridPos": {"x": 12, "y": 24, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "process_resident_memory_bytes",
            "legendFormat": "Memory",
            "refId": "A"
          },
          {
            "expr": "rate(process_cpu_seconds_total[5m])",
            "legendFormat": "CPU",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "bytes", "label": "Memory"},
          {"format": "percentunit", "label": "CPU"}
        ]
      },
      {
        "id": 11,
        "title": "Agent Activity",
        "type": "table",
        "gridPos": {"x": 0, "y": 32, "w": 24, "h": 8},
        "targets": [
          {
            "expr": "topk(10, sum by (agent_id) (rate(agentmem_requests_total[5m])))",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "styles": [
          {
            "pattern": "agent_id",
            "type": "string",
            "alias": "Agent ID"
          },
          {
            "pattern": "Value",
            "type": "number",
            "alias": "Requests/sec",
            "decimals": 2
          }
        ]
      }
    ],
    "templating": {
      "list": [
        {
          "name": "datasource",
          "type": "datasource",
          "query": "prometheus",
          "current": {
            "text": "Prometheus",
            "value": "Prometheus"
          }
        },
        {
          "name": "agent_id",
          "type": "query",
          "datasource": "$datasource",
          "query": "label_values(agentmem_requests_total, agent_id)",
          "refresh": 1,
          "multi": true,
          "includeAll": true
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "datasource": "$datasource",
          "enable": true,
          "expr": "ALERTS{alertstate=\"firing\"}",
          "iconColor": "red",
          "name": "Alerts",
          "step": "60s",
          "tagKeys": "alertname",
          "textFormat": "{{alertname}}",
          "titleFormat": "Alert"
        }
      ]
    },
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    }
  }
}

